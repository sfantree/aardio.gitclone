			var tagId = __event._handlerByTagRegistry.reg(handler);
			var ret = ::Sciter.AttachEventHandler(owner[["handle"]], __event._handlerByTagId_c,tagId ); 
			 
			if( ret == 0/*_HLDOM_OK*/){
				if( hOwner ){
					owner[["attachEventHandlerId"]] = tagId;
