					}
				}
				param_c = {struct array[] = postData }
				len = #postData;
				if(!len) { param_c = {} };
			}
			else {
				param_c = {};
				if(rt===null) rt = "GET";
				len = 0;
			};
			return 0 == ::Sciter.HttpRequest( owner[["handle"]],toUnicode(uri),dt,rt!="GET"?1:0,param_c,len);
